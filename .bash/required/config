# vim: set filetype=sh : ~

# Bash Prompt Escape Sequences
export PS1='\[\033[01;32m\]\u@\h\[\033[01;34m\] \w \$\[\033[00m\] '

# http://unix.stackexchange.com/questions/72086
# http://stackoverflow.com/questions/24623021
[[ $- == *i* ]] && stty -ixon


# https://github.com/Jarred-Sumner/bun
BUN_INSTALL="/Users/vv/.bun"
PATH="$BUN_INSTALL/bin:$PATH"
